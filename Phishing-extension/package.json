{
  "name": "phishguard-pro",
  "version": "1.0.0",
  "description": "Advanced phishing protection browser extension",
  "main": "background.js",
  "scripts": {
    "build": "npm run lint && npm run test",
    "lint": "eslint *.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "package": "zip -r phishguard-pro.zip . -x '*.git*' 'node_modules/*' '*.log' '*.tmp' 'tests/*' 'coverage/*'",
    "dev": "web-ext run --source-dir=.",
    "watch": "web-ext run --source-dir=. --watch"
  },
  "keywords": [
    "phishing",
    "security",
    "browser-extension",
    "cybersecurity"
  ],
  "author": "PhishGuard Pro Team",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-chrome": "^0.8.0",
    "jest-fetch-mock": "^3.0.3",
    "web-ext": "^7.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "commonjs"
}
